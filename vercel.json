{
  "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://picket-fe-deploy.vercel.app/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://picket-fe-deploy.vercel.app/:path*/"
    }
  ]
}
